<?xml version="1.0" ?>
<!-- Configuration of the Ant build system to generate a Jar file --> 
<project name="SpiddekaugaUtils" default="CreateSpiddekaugaUtilsJar">
  <target name="CreateSpiddekaugaUtilsJar" description="Create Jar file">
        <jar jarfile="dist/SpiddekaugaUtils.jar">
        	<fileset dir="bin" />
        </jar>
        <jar jarfile="dist/SpiddekaugaUtils-src.jar">
  			<fileset dir="src" includes="**/*.java" />
        </jar>
  </target>
</project>