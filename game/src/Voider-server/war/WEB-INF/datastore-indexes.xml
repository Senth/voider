	<!-- Explore — Sort by play count -->
	<datastore-index kind="level_stat" ancestor="true" source="manual">
		<property name="play_count" direction="desc"/>
	</datastore-index>
	
	<!-- Explore — Sort by rating -->
	<datastore-index kind="level_stat" ancestor="true" source="manual">
