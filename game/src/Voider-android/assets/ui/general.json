com.badlogic.gdx.graphics.g2d.BitmapFont: {
	default_font: { file: default.fnt }
},

/*
 * Colors are in RGBA where the values are in the range of 0.0 to 1.0 i.e. R value of 1 is 255
 * and value 0.5 is 127). If you want to convert the color original RBG value to the range of 0-1
 * just divide it with 255. I.e. 70 / 255 = ~0.275
 */
com.badlogic.gdx.graphics.Color: {
	// Color of the upper and lower bars, see Float.bar_upper_lower_height for height
	bar_upper_lower_color: { r: 0.3, g: 0.3, b: 0.3, a: 1 },
	// Color used as background for various widgets
	widget_background_color: { r: 0.3, g: 0.3, b: 0.3, a: 1 },
	
	red: { a: 1, b: 0, g: 0, r: 1 },
	green: { a: 1, b: 0, g: 1, r: 0 },
	
	black: { a: 1, b: 0, g: 0, r: 0 },
	white: { a: 1, b: 1, g: 1, r: 1 },
	
	grey: { a: 1, b: 0.3, g: 0.3, r: 0.3 },
	grey_light: { a: 1, b: 0.8, g: 0.8, r: 0.8 },
	grey_dark: { a: 1, b: 0.2, g: 0.2, r: 0.2 },
	
	highlight: { a: 1, b: 1, g: 1, r: 0 },
},

java.lang.Float: {
	/* Explore / Load */
	
	// Width of info bar in explore and load screen. Info bar displays name, creator, etc.
	info_bar_width: {value: 240},
	// Tag bar width
	tag_bar_width: {value: 144},
	// Maximum size of enemies/bullets on load screen (i.e. the image button) 
	load_actor_size_max: {value: 80},
	
	
	/* Padding */
	
	// Currently used on each element in tab content
	padding_default: {value: 4},
	// Padding in windows so that the content doesn't start directly at margin
	padding_window_left_right: {value: 8},
	// Used to separate group of widgets, e.g. tool groups
	padding_separator: {value: 32},
	// Padding after a label before other text, slider, or text field. E.g. between : and [ in Name: [___]
	padding_after_label: {value: 8},
	// Outer padding
	padding_outer: {value: 4},
	// Inside padding
	padding_inside: {value: 12},
	
	
	/* Other */
	
	// Stardard icon and row height
	icon_row_height: {value: 32},
	// Height of upper and lower bars. See Color.bar_upper_lower_color for setting color
	bar_upper_lower_height: {value: 40},
	// Width of numeric text fields (e.g. after a slider)
	text_field_number_width: {value: 64},
	// Fade in/out duration for wait window. Wait window displays loading icon or progress bar
	wait_window_fade_in: {value: 0.2},
	wait_window_fade_out: {value: 0.6},
},

