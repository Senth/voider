<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>AppEngineServletPrototype</servlet-name>
		<servlet-class>com.spiddekauga.prototype.AppEngineServletPrototypeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AppEngineServletPrototype</servlet-name>
		<url-pattern>/appengineservletprototype</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>NewUser</servlet-name>
		<servlet-class>com.spiddekauga.prototype.NewUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewUsers</servlet-name>
		<url-pattern>/new-users</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CreateHighscores</servlet-name>
		<servlet-class>com.spiddekauga.prototype.CreateHighscores</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateHighscores</servlet-name>
		<url-pattern>/create-highscores</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetUploadUrl</servlet-name>
		<servlet-class>com.spiddekauga.prototype.GetUploadUrl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetUploadUrl</servlet-name>
		<url-pattern>/getuploadurl</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
